class Solution:
    def twoSum(self, arr: List[int], target: int) -> List[int]:
        n = len(arr)
        i = 0
        j = n - 1
        for k in range(n):
            if arr[i] + arr[j] == target:
                return [i+1, j+1]
            elif arr[i] + arr[j] < target:
                i+=1
            else:
                j-=1
            
