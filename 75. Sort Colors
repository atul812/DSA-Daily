class Solution:
    def sortColors(self, nums: List[int]) -> None:
        left = 0
        right = len(nums)-1
        mid = 0

        while(mid<=right):
            if nums[mid] == 0:
                self.swap(nums, mid, left)
                left +=1
                mid +=1

            elif nums[mid] == 2:
                self.swap(nums, mid, right)
                right-=1
                

            else:
                mid +=1

        return nums

    def swap(self, nums, left, right):
        temp = nums[left]
        nums[left] = nums[right]
        nums[right] = temp

        






        
