class Solution:
    def isSubsequence(self, s: str, t: str) -> bool:
        str1 = list(s)
        str2 = list(t)
        i=0
        #for empty string s
        if str1==[]: 
            return True
        for j in range(len(str2)):
            if str1[i]==str2[j]:
                i+=1
                if i==len(str1): break #if i reaches at the end of the list, break
        if i==len(str1):
            return True
        else:
            return False
